            </MenuItem>
            <MenuItem Header="Gestion Métier">
                <MenuItem Header="Domaine métier" />
                <MenuItem Header="Métier" />
            </MenuItem>
            <MenuItem Header="Gestion Pack">
            </MenuItem>
        </Menu>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="385" Margin="0,24,0,0" VerticalAlignment="Top" Width="400">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>


    </Grid>
</Window>
