        <TextBox x:Name="TxtBMdp" HorizontalAlignment="Left" Height="23" Margin="260,272,0,0" TextWrapping="Wrap" Text="Adresse" VerticalAlignment="Top" Width="130"/>
        <Button x:Name="BtAddNbrPost" Content="+" HorizontalAlignment="Left" Height="23" Margin="135,373,0,0" VerticalAlignment="Top" Width="25" RenderTransformOrigin="0.03,-0.489" Click="BtAddNbrPost_Click"/>

